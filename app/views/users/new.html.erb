<%= form_for @user do |f| %>
  <%= f.text_field :full_name, placeholder: "Full Name" %>
  <%= f.text_field :display_name, placeholder: "Display Name" %>
  <%= f.text_field :email, placeholder: "Email" %>
  <%= f.password_field :password, placeholder: "Password" %>
  <%= f.submit "Create Account" %>
<% end %>
